    <record id="view_estado_civil_tree" model="ir.ui.view">
      <field name="name">view.estadocivil.tree</field>
      <field name="model">estado.civil</field>
      <field name="arch" type="xml" >
	    <tree string="Estado Civil">
            <field name="name"/>
	    </tree>
      </field>
