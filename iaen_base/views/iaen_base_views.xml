     <!-- Ingreso de cargas familiar -->
     <record id="view_family_burden_tree" model="ir.ui.view">
          <field name="name">view.family.burden.tree</field>
          <field name="model">family.burden</field>
          <field name="arch" type="xml">
               <tree string="Carga Familiar">
                    <field name="name"/>
                    <field name="lastName"/>
