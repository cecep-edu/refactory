          </field>
        </record>

        <record id="view_canton_form" model="ir.ui.view">
          <field name="name">view.canton.form</field>
          <field name="model">canton</field>
          <field name="arch" type="xml">
            <form string="Cantones">
              <field name="name"/>
              <newline/>
              <field name="country_id"/>
              <newline/>
              <field name="description"/>
            </form>
          </field>
        </record>


