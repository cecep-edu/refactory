<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>

    <record id="view_estadocivil_tree" model="ir.ui.view">
      <field name="name">view.estadocivil.tree</field>
      <field name="model">estado.civil</field>
      <field name="arch" type="xml" >
	    <tree string="Estado Civil">
            <field name="name"/>
	    </tree>
      </field>
    </record>

    <record id="view_estadocivil_form" model="ir.ui.view">
      <field name="name">view.estadocivil.form</field>
      <field name="model">estado.civil</field>
      <field name="arch" type="xml">
	    <form string="Crear Estado Civil">
	          <field name="name" />
	    </form>
      </field>
    </record>

	<record id="view_blood_type_tree" model="ir.ui.view">
      <field name="name">view.blood_type.tree</field>
      <field name="model">blood.type</field>
      <field name="arch" type="xml">
	    <tree string="Tipos de Sangre">
	      <field name="name"/>
	    </tree>
      </field>
    </record>	
    
    <record id="view_blood_type_form" model="ir.ui.view">
      <field name="name">view.blood_type.form</field>
      <field name="model">blood.type</field>
      <field name="arch" type="xml">
	    <form string="Tipos de Sangre">
          <group  col="4" colspan="2">
            <group colspan="2">
	          <field name="name"/>
            </group>
          </group>
	    </form>
      </field>
    </record>
	<!-- Ingreso de tipos de identificaciones -->
	<record id="view_identification_type_tree" model="ir.ui.view">
		<field name="name">view.identification.type.tree</field>
		<field name="model">identification.type</field>
		<field name="arch" type="xml">
			<tree string="Tipo de Identificación">
				<field name="name"/>
				<field name="description"/>
			</tree>
		</field>
	</record>

	<record id="view_identification_type_form" model="ir.ui.view">
		<field name="name">view.identification.type.form</field>
		<field name="model">identification.type</field>
		<field name="arch" type="xml">
			<form string="Tipo de Identificación">
				<group col="2" colspan="2">
					<field name="name"/>
					<field name="description"/>
				</group>
			</form>
		</field>
	</record>

