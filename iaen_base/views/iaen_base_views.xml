	  <field name="arch" type="xml">
		<form string="Tipos de Sangre" version="7.0">
          <sheet>
		    <group  col="4" colspan="2">
			  <group colspan="2">
			    <field name="name"/>
			  </group>
		    </group>
          </sheet>
		</form>
	  </field>
	</record>

